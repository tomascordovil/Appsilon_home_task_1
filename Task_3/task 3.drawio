        <mxCell id="WAaLvcebSka8-u2p9-O3-32" value="GitHub:&lt;div&gt;- Used for version control&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="1">
